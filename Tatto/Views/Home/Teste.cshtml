@{ ViewData["Title"] = "Contrato para Menor";
    Layout = "/Views/Shared/_LayoutAdminEstudio.cshtml";}

@model Tatto.Models.Estudio

<style>
    h3 {
        text-align: center;
    }

    p {
        text-align: justify;
        font-size: 20px;
        line-height: 2.5;
    }

    #end {
        text-align: center;
    }

    span {
        font-weight: bold;
        background-color: mistyrose;
        text-decoration: underline;
        padding: 8px;
    }
</style>

@section Scripts
{
    <script src="~/js/cep.js"></script>
    <script src="~/js/tatto.js"></script>

    <script src="~/js/jcrop/jquery.Jcrop.min.js"></script>
    <script src="~/js/ImageCrop.js"></script>

    <script src="~/js/jquery.maskedinput.js"></script>

    <script type="text/javascript">

        function myPrint(myfrm) {
            var printdata = document.getElementById(myfrm);
            newwin = window.open("");
            newwin.document.write(printdata.outerHTML);
            newwin.print();
            newwin.close();
        }

    </script>




}


<link href="~/assets/css/navdark.css" rel="stylesheet">
